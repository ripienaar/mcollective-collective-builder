topicprefix = /topic/<%= collective %>
libdir = <%= instance_home %>/plugins
rpchelptemplate = <%= instance_home %>/etc/rpc-help.erb
logfile = <%= BASEDIR %>/logs/<%= identity %>-client.log
loglevel = debug
daemonize = 1
identity = <%= identity %>

# Plugins
securityprovider = none

connector = stomp
plugin.stomp.pool.size = 1
plugin.stomp.pool.host1 = <%= stompserver %>
plugin.stomp.pool.port1 = <%= stompport %>
plugin.stomp.pool.user1 = <%= stompuser %>
plugin.stomp.pool.password1 = <%= stomppass %>
plugin.stomp.pool.ssl1 = <%= stompssl == "y" ? "true" : "false" %>
